{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true
  },
  // aliases
  "baseUrl": ".",
  "paths": {
    "@/*": ["src/*"],
    "@config/*": ["config/*"],
    "@util": ["util"]
  },
  "include": ["vite.config.ts", "typings/**/*", "config/**/*"]
}
